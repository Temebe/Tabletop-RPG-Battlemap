<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox id="VBox" mouseTransparent="false" prefHeight="600.0" prefWidth="900.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controllers.OfflineMapEditorController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#popUpNewMapSettings" text="New" />
            <MenuItem mnemonicParsing="false" text="Open…" />
            <Menu mnemonicParsing="false" text="Open Recent" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Close" />
            <MenuItem mnemonicParsing="false" onAction="#saveMap" text="Save" />
            <MenuItem mnemonicParsing="false" onAction="#saveMapAs" text="Save As…" />
            <MenuItem mnemonicParsing="false" text="Revert" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Preferences…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#undo" text="Undo" fx:id="undoBtn" />
            <MenuItem mnemonicParsing="false" onAction="#redo" text="Redo" fx:id="redoBtn" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Cut" />
            <MenuItem mnemonicParsing="false" text="Copy" />
            <MenuItem mnemonicParsing="false" text="Paste" />
            <MenuItem mnemonicParsing="false" text="Delete" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Select All" />
            <MenuItem mnemonicParsing="false" text="Unselect All" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Resources">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#loadZipPackageWindow" text="Add resource pack from zip" />
            <MenuItem mnemonicParsing="false" onAction="#updatePackages" text="Refresh resources" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <ToolBar>
      <items>
        <ToggleButton fx:id="drawToolButton" mnemonicParsing="false" onAction="#setActionDrawing" selected="false" text="Draw" underline="false">
          <toggleGroup>
            <ToggleGroup fx:id="mainToolbar" />
          </toggleGroup>
        </ToggleButton>
        <ToggleButton mnemonicParsing="false" onAction="#setActionRuler" text="Ruler" toggleGroup="$mainToolbar" />
      </items>
    </ToolBar>
    <SplitPane fx:id="splitPane" blendMode="SRC_OVER" disable="false" dividerPositions="0.34966592427616927" focusTraversable="true" onDragOver="#updateTilesLayout" prefHeight="-1.0" prefWidth="-1.0" visible="true" VBox.vgrow="ALWAYS">
      <items>
        <AnchorPane fx:id="tilesChooseView" blendMode="DARKEN" prefHeight="547.0" prefWidth="306.9998779296875">
          <children>
            <ChoiceBox fx:id="packageChoiceBox" blendMode="OVERLAY" layoutX="14.0" layoutY="14.0" onContextMenuRequested="#changePackage" prefWidth="283.0">
              <items>
                <FXCollections fx:factory="observableArrayList" />
              </items>
            </ChoiceBox>
          </children>
        </AnchorPane>
        <ScrollPane pickOnBounds="true" prefHeight="-1.0" prefWidth="-1.0">
          <content>
            <AnchorPane id="Content" fx:id="mapView" focusTraversable="false" minHeight="-1.0" minWidth="-1.0" onMouseDragged="#moveCamera" prefHeight="545.0" prefWidth="840.0" visible="true" />
          </content>
        </ScrollPane>
      </items>
    </SplitPane>
    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
      <children>
        <Label maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Left status" HBox.hgrow="ALWAYS">
          <font>
            <Font size="11.0" fx:id="x3" />
          </font>
          <textFill>
            <Color blue="0.625" green="0.625" red="0.625" fx:id="x4" />
          </textFill>
        </Label>
        <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <Label font="$x3" maxWidth="-1.0" text="Right status" textFill="$x4" HBox.hgrow="NEVER" />
      </children>
      <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </padding>
    </HBox>
  </children>
</VBox>
